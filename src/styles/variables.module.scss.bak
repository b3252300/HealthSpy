// 可在此处定义主题色
:root {
  --el-color-primary: #25c168;
  // --el-color-success: red;
  --el-text-color-primary:#222222;
  --root-header-info-bar-height: 60px;
}

.el-dialog__header{
  text-align: center;
}

.el-dialog__body{
  text-align: center;
  
}

// base color
$blue: #324157;
$light-blue: #3a71a8;
$red: #c03639;
$pink: #e65d6e;
$green: #30b08f;
$tiffany: #4ab7bd;
$yellow: #fec171;
$panGreen: #30b08f;

// sidebar
$menuActiveText: #ffd04b;
$menuBg: #545c64;
$menuText: #fff;
$subMenuHover: #8c9297;
$subMenuBg: #1f2d3d;

$subMenuActiveText: #f4f4f5; // https://github.com/ElemeFE/element/issues/12951
$menuHover: #263445;
$sideBarWidth: 210px;

// the :export directive is the magic sauce for webpack
// https://www.bluematador.com/blog/how-to-share-variables-between-js-and-sass
:export {
  menuActiveText: $menuActiveText;
  menuBg: $menuBg;
  menuText: $menuText;
  subMenuHover: $subMenuHover;
  subMenuBg: $subMenuBg;

  subMenuActiveText: $subMenuActiveText;
  menuHover: $menuHover;
  sideBarWidth: $sideBarWidth;
}
